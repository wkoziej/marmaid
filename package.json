{
  "name": "marmaid",
  "version": "1.0.0",
  "description": "Marmaid - Visualization tool for Marma points for therapists",
  "private": true,
  "scripts": {
    "setup": "./scripts/setup-development.sh",
    "dev": "./scripts/start-development.sh",
    "dev:reset": "supabase db reset && cd frontend && npm run dev",
    "test": "cd frontend && npm run test",
    "test:unit": "cd frontend && npm run test:unit", 
    "test:integration": "cd frontend && npm run test:integration",
    "test:coverage": "cd frontend && npm run test:coverage",
    "build": "cd frontend && npm run build",
    "lint": "cd frontend && npm run lint",
    "typecheck": "cd frontend && npm run typecheck",
    "quality-gates": "cd frontend && npm run quality-gates",
    "env:test": "./scripts/use-test.sh",
    "env:prod": "./scripts/use-production.sh",
    "env:local": "./scripts/use-local.sh",
    "db:migrate": "supabase migration new",
    "db:push:test": "supabase link --project-ref myxicttnpflkwnofbhci && supabase db push",
    "db:push:prod": "supabase link --project-ref aajurxtbngbixsdptfzz && supabase db push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/marmaid.git"
  },
  "keywords": [
    "marma",
    "therapy",
    "visualization",
    "react",
    "supabase"
  ],
